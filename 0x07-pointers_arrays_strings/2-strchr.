# 2 "2-strchr.c" 2
# 10 "2-strchr.c"
char *_strchr(char *s, char c)
{
 int i;

 for (i = 0; s[i] != '\0'; i++)
 {
  if (s[i] == c)
  {
   return (s + i);
  }
 }
 if (s[i] == c)
 {
  return (s + i);
 }
}
